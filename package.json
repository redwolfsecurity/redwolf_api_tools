{
  "name": "@ff/ff_api_tools",
  "version": "2.8.8",
  "description": "A set of tools to interact with the framework API's",
  "main": "dist/index.js",
  "type": "commonjs",
  "bin": {},
  "scripts": {
    "start": "node dist/index.js",
    "container_run": "npm run build &&  node ./examples/20_messaging/14a_remote_message_sniffer.js",
    "container_start": "npm run container_stop; npm run container_build && node bin/container_start.js",
    "container_stop": "node bin/container_stop.js",
    "container_build": "npm run container_prune && node bin/container_build.js",
    "container_prune": "node bin/container_prune.js",
    "build": "npm run test:quality && npm run tsc_incremental",
    "repl": "node examples/repl.js",
    "test:quality": "npm run tslint_check",
    "tsc": "tsc",
    "tsc_incremental": "tsc --build --incremental",
    "tsc_force": "tsc --build --force",
    "tslint_check": "tslint 'src/**/*.ts?(x)'",
    "elint": "npx eslint src",
    "terser": "terser lib/ff_api_tools.js --ecma 6 -o lib/ff_api_tools.min.js",
    "webpack": "npx webpack --config webpack.config.js",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:redwolfsecurity/fancy_framework.git"
  },
  "keywords": [],
  "author": "RedOKI Incorporated",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/redwolfsecurity/redwolf_api_tools/issues"
  },
  "homepage": "https://github.com/redwolfsecurity/redwolf_api_tools#readme",
  "files": [
    "lib",
    "dist",
    "src",
    "test",
    "examples",
    "node_modules"
  ],
  "dependencies": {
    "@ff/fancy_framework": "https://cdn.redwolfsecurity.com/ff/npm/ff-fancy_framework-latest.tgz",
    "@ff/ff_http_request": "^2.8.10",
    "commander": "^9.0.0",
    "json-nested-replace": "^1.0.4",
    "recursive-key-replace": "^1.0.0"
  },
  "devDependencies": {
    "@types/debug": "^4.1.4",
    "@types/jest": "^26.0.23",
    "@types/jquery": "^3.5.5",
    "@types/node": "^11.13.10",
    "@types/uuid": "^3.4.5",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "axios-mock-adapter": "^1.17.0",
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "chai-events": "0.0.1",
    "chai-match": "^1.1.1",
    "chai-spies": "^1.0.0",
    "dependency-cruiser": "^10.0.1",
    "eslint": "^7.29.0",
    "express": "^4.17.1",
    "jest": "^27.5.1",
    "jest-puppeteer": "^5.0.4",
    "mocha": "^9.1.3",
    "mock-fs": "^4.10.3",
    "puppeteer": "9.1.1",
    "source-map-support": "^0.5.19",
    "terser": "^5.7.0",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^4.5.5",
    "uglify-es": "^3.3.9",
    "webpack-cli": "^4.8.0",
    "why-is-node-running": "^2.2.0"
  }
}
